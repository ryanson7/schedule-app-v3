{
  "name": "schedule-app-v3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run menu:generate && next dev -p 3000",
    "dev:stable": "next dev -p 3000",
    "dev:turbo": "next dev --turbo -p 3000",
    "build": "next build",
    "menu:generate": "node scripts/generate-menu.js",
    "start": "NODE_ENV=production next start -p 3000",
    "clean": "rimraf .next node_modules/.cache .eslintcache",
    "fresh": "npm run clean && npm ci && npm run dev",
    "fresh:webpack": "npm run clean && npm ci && npm run dev:webpack",
    "fresh:turbo": "npm run clean && npm ci && npm run dev:turbo",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "echo \"No tests specified\" && exit 0",
    
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.8.0",
    "@supabase/supabase-js": "^2.0.0",
    "@types/qrcode": "^1.5.5",
    "@yudiel/react-qr-scanner": "^2.3.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "firebase": "^11.10.0",
    "formidable": "^3.5.4",
    "html5-qrcode": "^2.3.8",
    "kakaowork-api": "^1.1.9",
    "next": "^15.5.2",
    "next-auth": "^4.24.11",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-csv": "^2.2.2",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.5.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@next/bundle-analyzer": "^14.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/xlsx": "^0.0.35",
    "critters": "^0.0.23",
    "cross-env": "^7.0.3",
    "eslint": "8.57.1",
    "eslint-config-next": "15.4.6",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.0.0"
  }
}
